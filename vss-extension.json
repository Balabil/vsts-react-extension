{
    "manifestVersion": 1,
    "id": "mw-dashboard",
    "publisher": "NabilGhayyda",
    "version": "0.1",
    "name": "Monthly PR Report",
    "description": "A Visual Studio Services extension for MultiWeb's purposes",
    "public": false,
    "categories": [
        "Azure Repos"
    ],
    "scopes": [
        "vso.work",
        "vso.code_write",
        "vso.build_execute",
        "vso.build",
        "vso.code"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "files": [
        {
            "path": "build",
            "addressable": true,
            "packagePath": "/"
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "mw-pr",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "Monthly PR Report",
                "uri": "index.html"
            }
        }
    ]
}